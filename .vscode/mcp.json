{
  "servers": {
    // https://github.com/microsoft/playwright-mcp
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--caps=vision",
        "--browser=chrome",
        "--extension"
      ]
    },
    "mui-mcp": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@mui/mcp@latest"
      ]
    }
  }
}
